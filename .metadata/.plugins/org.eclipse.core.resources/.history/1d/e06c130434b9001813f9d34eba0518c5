package phoneBook.businessObjects;

public class Group implements IGroup {
	
	private Integer id;
	private String name;
	private String description;
	
	/* (non-Javadoc)
	 * @see phoneBook.businessObjects.IGroup#getId()
	 */
	public Integer getId() {
		return id;
	}
	
	/* (non-Javadoc)
	 * @see phoneBook.businessObjects.IGroup#setId(java.lang.Integer)
	 */
	public void setId(Integer id) {
		this.id = id;
	}
	
	/* (non-Javadoc)
	 * @see phoneBook.businessObjects.IGroup#getName()
	 */
	public String getName() {
		return name;
	}
	
	/* (non-Javadoc)
	 * @see phoneBook.businessObjects.IGroup#setName(java.lang.String)
	 */
	public void setName(String name) {
		this.name = name;
	}
	
	/* (non-Javadoc)
	 * @see phoneBook.businessObjects.IGroup#getDescription()
	 */
	public String getDescription() {
		return description;
	}
	
	/* (non-Javadoc)
	 * @see phoneBook.businessObjects.IGroup#setDescription(java.lang.String)
	 */
	public void setDescription(String description) {
		this.description = description;
	}
	

}
